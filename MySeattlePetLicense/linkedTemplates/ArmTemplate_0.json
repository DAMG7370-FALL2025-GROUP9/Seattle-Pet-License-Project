								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "CSV_ds",
								"type": "DatasetReference",
								"parameters": {
									"container_name": "stagedata",
									"folder_name": "rawdata",
									"file_name": "geo-data.csv"
								}
							}
						],
						"outputs": [
							{
								"referenceName": "Parquet_ds",
