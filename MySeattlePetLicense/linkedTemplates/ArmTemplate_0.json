				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/CSV_ds')]",
				"[concat(variables('factoryId'), '/datasets/Parquet_ds')]"
			]
